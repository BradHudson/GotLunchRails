<p id="notice"><%= notice %></p>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Vote</th>
      <th>Date</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @votes.each do |vote| %>
      <tr>
        <td><%= vote.name %></td>
        <td><%= vote.vote %></td>
        <td><%= vote.date %></td>
      </tr>
    <% end %>
  </tbody>
</table>
